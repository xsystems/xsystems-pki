client
remote-cert-tls server
remote 0.router.xsystems.org 1194

dev tun
proto udp

nobind
user nobody
group nobody

persist-key
persist-tun

mute-replay-warnings
log openvpn.log
verb 3

tls-client
tls-version-min 1.2
tls-cipher TLS-ECDHE-ECDSA-WITH-AES-256-GCM-SHA384
cipher AES-256-GCM
auth SHA512
